package BOJ_NM6_15655;

import java.io.File;
import java.io.FileNotFoundException;
import java.util.Arrays;
import java.util.Scanner;

// 중복 없는 조합
public class Main {
	static int[] list; // 원본 리스트
	static int[] result; // 결과 리스트
	static int R; // 추출할 숫자 수
	static int N; // 전체 원소 수
	public static void main(String[] args) throws FileNotFoundException {
		File file = new File("./src/BOJ_NM6_15655/input.txt");
		Scanner sc = new Scanner(file);
		Scanner sc = new Scanner(System.in);
		
		// #### 데이터 입력 ####
		N = sc.nextInt(); // 전체 원소 수
		R = sc.nextInt(); // 추출할 숫자 수
		list = new int[N];
		for (int i = 0; i < N; i++) {
			list[i] = sc.nextInt();
		} // 원본 리스트
		
		// #### 원본 리스트 원소 정렬 ####
		Arrays.sort(list);
		
		// #### 기본 변수 정의 ####
		result = new int[R];
		
		// #### combination 함수 출력 ####
		combination(0, 0);
	} // main
	
	public static void combination(int start, int idx) {
		// 1. 탈출 조건
		if (idx == R) {
			// 1-1. 결과 원소 추출
			for (int r : result) {
				System.out.print(r + " ");
			}
			System.out.println();
			return;
		}
		// 2. start 지점부터 원소 탐색
		for (int i = start; i < N; i++) {
			// 2-1. 원소 담기
			result[idx] = list[i];
			// 2-2. 다음에 올 원소 탐색 (재귀)
			combination(i + 1, idx + 1);
		}
	}

}
